<dl class="php method">
    <dt class="sig sig-object php" id="{{ node.id }}">
        {%- for modifier in node.modifiers -%}
            {{- renderNode(modifier) }}{{ ' ' -}}
        {%- endfor -%}
        {{- renderNode(node.methodName) -}}
    </dt>
    <dd>
        {{ renderNode(node.value) }}
    </dd>
</dl>
