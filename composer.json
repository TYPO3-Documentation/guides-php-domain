{
    "name": "t3docs/guides-php-domain",
    "description": "PHP domain directives for Restructured Text",
    "license": "MIT",
    "type": "library",
    "homepage": "https://docs.typo3.org",
    "require": {
        "phpdocumentor/guides": "^1.0",
        "phpdocumentor/guides-restructured-text": "^1.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.39",
        "friendsofphp/php-cs-fixer": "^3.39",
        "phpdocumentor/guides-cli": "^0.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10.56",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^10.4",
        "rector/rector": "0.19.1",
        "symplify/phpstan-rules": "^12.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "T3Docs\\GuidesPhpDomain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T3Docs\\GuidesPhpDomain\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
