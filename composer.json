{
    "name": "t3docs/guides-php-domain",
    "description": "PHP domain directives for Restructured Text",
    "license": "MIT",
    "type": "library",
    "homepage": "https://docs.typo3.org",
    "require": {
        "phpdocumentor/guides": "dev-main",
        "phpdocumentor/guides-restructured-text": "dev-main"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.39",
        "friendsofphp/php-cs-fixer": "^3.39",
        "phpdocumentor/guides-cli": "dev-main",
        "phpunit/phpunit": "^10.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "T3Docs\\GuidesPhpDomain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T3Docs\\GuidesPhpDomain\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    }
}
